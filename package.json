{
  "name": "nvim-codeshot",
  "version": "1.0.0",
  "description": "Node.js bindings for neovim codeshot plugin.",
  "type": "module",
  "scripts": {
    "build": "node --loader ts-node/esm --no-warnings scripts/build.ts",
    "clean": "rm -rf $HOME/.cache/puppeteer > /dev/null 2>&1",
    "debug": "node --loader ts-node/esm --no-warnings",
    "format": "prettier -w .",
    "install": "node bin/install.cjs",
    "screenshot": "node bin/screenshot.cjs",
    "types": "tsc --noEmit",
    "watch": "nodemon"
  },
  "author": {
    "name": "Szymon Jaworski",
    "url": "https://github.com/Ofadiman"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.27",
    "@types/which": "^3.0.3",
    "esbuild": "0.20.1",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@puppeteer/browsers": "^2.1.0",
    "change-case": "^5.4.3",
    "consola": "^3.2.3",
    "execa": "^8.0.1",
    "puppeteer": "^22.5.0",
    "shiki": "^1.2.0",
    "which": "^4.0.0",
    "zod": "^3.22.4"
  },
  "volta": {
    "node": "20.14.0",
    "npm": "10.8.1"
  }
}
